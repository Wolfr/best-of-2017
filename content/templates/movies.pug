extends index

block append pageVariables
    - var pageTitle = "Movies"

block maincontent
    .cards
        each item in contentData.movies.records
            if item.fields.name
                .card.card-2.js-tilt
                    +image(item)
                    .card-body
                        h3.card-heading #{item.fields.name}
                        p=moment(item.fields.releaseDate).format("MMMM Do")
